{
  "name": "vue-todo-app",
  "version": "1.0.0",
  "description": "vue todo app",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.conf.js",
    "report": "cross-env NODE_ENV=production webpack --config ./build/webpack.report.conf.js",
    "test": "mochapack --webpack-config ./build/webpack.prod.conf.js --require test/setup.js ./test/**/*.spec.js",
    "clear": "rimraf ./dist"
  },
  "keywords": [
    "vue-todo",
    "todo-list"
  ],
  "author": "XueDong Wang",
  "license": "MIT",
